 <% if @visit.nil? || @visit.checkin == false %>
  <%= link_to "Checkin",[@workplace, :visits], class: "waves-effect waves-light btn" , :method => :post, remote: true %>
  <% else %>
  <%= link_to "Checkout",[@workplace, @visit], class: "waves-effect waves-light btn" , :method => :put, remote: true %>
<% end %>
